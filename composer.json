{
    "name": "goetas/to-swift-mime-parser",
    "description": "Parse a generic mail stream, and convert it to a SwiftMailer Message",
    "keywords": ["swift", "mail", "mime"],
    "homepage": "https://github.com/goetas/to-swift-mime-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Asmir Mustafic",
            "email": "goetas@lignano.it"
        }
    ],
    "require": {
        "php": "^5.3.2|^7.0",
        "swiftmailer/swiftmailer": "^4.2|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-0": { "Goetas\\Mail\\ToSwiftMailParser": "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Goetas\\Mail\\ToSwiftMailParser\\Tests\\": "tests/" }
    },
    "extra":{
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
